r\n        )\r\n        .sort((current, next) => { console.log( '---' ); console.dir(current); console.dir(next); return next.stock - current.stock });\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n#app {\r\n  height: 100%;\r\n  width: calc(100% - 2*100px);\r\n  padding: 0 100px;\r\n  position: absolute;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n\r\n  .btn-position {\r\n    margin-top: 60px;\r\n    margin-bottom: 30px;\r\n  }\r\n}\r\n</style>\r\n\r\n<style>\r\nbody {\r\n  margin: 0;\r\n}\r\n</style>\r\n"],"sourceRoot":"src"}]}